//Identificar si el numero ingresado es 5 o 6

import java.util.Scanner;

public class 5OR6 {

    public static void main(String[] args){
        Scanner input = new Scanner(System.in);

        System.out.println("Ingrese un número");
        int num = input.nextInt();

        if (num=5){
            System.out.println("El número no es el 5.");
        } 
        else if (num=6) {
            System.out.println("El número no es el 6.");
        }
        else{
            System.out.println("El número no es ni 5 ni 6.");
        }

    }
}
